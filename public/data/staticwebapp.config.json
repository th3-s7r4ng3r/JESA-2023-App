{
  "trailingSlash": "always",
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "routes": [
    {
      "route": "/award/data/*",
      "rewrite": "/data/*"
    },
    {
      "route": "/award/images/*",
      "rewrite": "/images/*"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
